---
import "normalize.css";
import BasicHtmlHead from "../components/BasicHtmlHead.astro";
import "../styles/app.sass";
import "../styles/article.sass";
import "../styles/raster.css";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <BasicHtmlHead title={title + " | Nathan's Blog"} />
  <body>
    <slot />
  </body>
</html>
