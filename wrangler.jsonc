{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "zartre-2024",
  "compatibility_date": "2025-06-29",
  "assets": {
    "directory": "./dist",
    "not_found_handling": "404-page"
  },
  "observability": {
    "logs": {
      "enabled": true
    }
  },
  "routes": [
    {
      "pattern": "zartre.com",
      "custom_domain": true
    }
  ],
  "vars": {
    "VITE_BLOG_URL": "/blog"
  }
}
